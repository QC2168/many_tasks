<block a:if="{{!disabled}}"><view class="u-upload data-v-11db79cb"><block a:for="{{lists}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{showUploadList}}"><view class="u-list-item u-preview-wrap data-v-11db79cb" style="{{'width:'+(width+'rpx')+';'+('height:'+(width+'rpx')+';')}}"><block a:if="{{deletable}}"><view data-event-opts="{{[['tap',[['deleteItem',[index]]]]]}}" class="u-delete-icon data-v-11db79cb" catchTap="__e"><u-icon class="u-icon data-v-11db79cb" vue-id="{{'3ed48b83-1-'+index}}" name="close" size="20" color="#ffffff" onVueInit="__l"></u-icon></view></block><block a:if="{{showProgress&&item.progress>0&&!item.error}}"><u-line-progress class="u-progress data-v-11db79cb" vue-id="{{'3ed48b83-2-'+index}}" show-percent="{{false}}" height="16" percent="{{item.progress}}" onVueInit="__l"></u-line-progress></block><block a:if="{{item.error}}"><view data-event-opts="{{[['tap',[['retry',[index]]]]]}}" class="u-error-btn data-v-11db79cb" catchTap="__e">点击重试</view></block><block a:if="{{!item.isImage}}"><image class="u-preview-image data-v-11db79cb" src="{{item.url||item.path}}" mode="{{imageMode}}" data-event-opts="{{[['tap',[['doPreviewImage',[item.url||item.path,index]]]]]}}" catchTap="__e"></image></block></view></block></block><slot name="file" file="{{lists}}"></slot><block a:if="{{maxCount>lists.length}}"><view data-event-opts="{{[['tap',[['selectFile',['$event']]]]]}}" style="display:inline-block;" onTap="__e" class="data-v-11db79cb"><slot name="addBtn"></slot><block a:if="{{!customBtn}}"><view class="u-list-item u-add-wrap data-v-11db79cb" style="{{'width:'+(width+'rpx')+';'+('height:'+(width+'rpx')+';')}}" hover-class="u-add-wrap__hover" hover-stay-time="150"><u-icon class="u-add-btn data-v-11db79cb" vue-id="3ed48b83-3" name="plus" size="40" onVueInit="__l"></u-icon><view class="u-add-tips data-v-11db79cb">{{uploadText}}</view></view></block></view></block></view></block>